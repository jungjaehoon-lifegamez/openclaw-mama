{
  "name": "@jungjaehoon/openclaw-mama",
  "version": "0.5.0",
  "description": "MAMA Memory Plugin for OpenClaw - Semantic decision memory with reasoning graph",
  "main": "index.ts",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ]
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "memory",
    "semantic-search",
    "decision-tracking",
    "mama"
  ],
  "author": "SpineLift Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jungjaehoon-lifegamez/openclaw-mama.git"
  },
  "bugs": {
    "url": "https://github.com/jungjaehoon-lifegamez/openclaw-mama/issues"
  },
  "homepage": "https://github.com/jungjaehoon-lifegamez/openclaw-mama#readme",
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "dependencies": {
    "@jungjaehoon/mama-core": "^1.1.5",
    "@sinclair/typebox": "^0.32.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
    "prebuild-install": "^7.1.0"
  },
  "files": [
    "index.ts",
    "utils.ts",
    "openclaw.d.ts",
    "openclaw.plugin.json",
    "scripts/",
    "README.md"
  ]
}
